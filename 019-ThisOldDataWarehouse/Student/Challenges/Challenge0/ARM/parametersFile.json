{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "dataFactoryName": {
      "value": "{Data Factory Name}"
    },
    "location": {
      "value": "[resourceGroup().location]"
    },
    "virtualNetworkName": {
      "value": "{Vnet Name}"
    },
    "subNetName": {
      "value": "default"
    },
    "nodeSize": {
      "value": "Standard_D2_v3"
    },
    "nodeNumber": {
      "value": 1
    },
    "maximumParallelExecutionsPerNode": {
      "value": 1
    },
    "azureSqlServerName": {
      "value": "{Database Server Name}"
    },
    "databaseAdminUsername": {
      "value": "{USERNAME}"
    },
    "databaseAdminPassword": {
      "value": "{PASSWORD}"
    },
    "catalogPricingTier": {
      "value": "Basic"
    },
    "databaseName": {
      "value": "{Database Name}"
    }
  }
}